group_age_list = [['00-04'] ,['05-14'] ,['15-34'], ['35-59'], ['60-79' ,'80-99']]
population = np.array([0.79+3 ,1+6.06+1,2.01+2.4+3.7+10.52,5.26+23.07,5.99+18.44])*10**6
